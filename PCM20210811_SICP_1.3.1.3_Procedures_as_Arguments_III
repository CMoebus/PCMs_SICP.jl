# ╔═╡ 67636a41-29d9-4b86-b245-4e0581ee3e88
md"
---
##### 3.4 *2nd* Variant of *Lebesgue-Integration*: Summation of *Simple Step* Functions
Our Julia script $lebesgueIntegralIter2$ sum *steps* according to the formula:

$\int_a^b f(x)\;dx = \int_a^b f(x)\;\mu(dx) =$

$\;$
$\;$
$\;$

$= \lim_{\Delta y \to 0} \left[\sum_{n=0}^{\lfloor max(f(x))/Δy \rfloor} n\Delta y \cdot \mu_{[a,b]}(\{x|n\Delta y) < f(x) \leq (n+1) \Delta y\})\right] =$

$\;$
$\;$
$\;$

$= \lim_{\Delta y \to 0} \left[\Delta y \sum_{n=0}^{\lfloor max(f(x))/Δy \rfloor} n \cdot \mu_{[a,b]}(\{x|n\Delta y) < f(x) \leq (n+1)\Delta y\})\right].$

$\;$
$\;$
$\;$

"

# ╔═╡ eb25e54c-7dd3-48be-b4df-92b3f1de3203
function plotLebesguesIntegralBySumOfSteps()
	#--------------------------------------------------------------------------------
	# plot of mixed density
	plot(mixedDensity, -10.0, 10, size=(700, 500), xlim=(-7.0, 10.0), ylim=(-0.05, 0.45), line=:darkblue, fill=(0, :lightblue), framestyle=:semi, title="2nd Lebesgue Integration (= Summation of Simple Steps)", titlefontsize=12, xlabel=L"$X$", ylabel=L"$f(X)$", label=L"$f(X)$")
	plot!(x->0.00, -5, 10, line=:darkblue, lw=1,label="")      # blue base line
	#--------------------------------------------------------------------------------
	# markings of integration limits a and b
	plot!([(-4.5,.00),(-4.5,.01)],line=:black,lw=1,label=L"a") # vertical line for a
	plot!([(+7.5,.00),(+7.5,.01)],line=:black,lw=1,label=L"b") # vertical line for b
	annotate!(-4.5, -0.01, L"a", 10)
	annotate!(+7.5, -0.01, L"b", 10)
	#--------------------------------------------------------------------------------
	# two horizontal stripes
	plot!(x->0.16, -5.5, 9, line=:darkblue, lw=1,label="") # 3rd blue horizontal line
	plot!(x->0.12, -5.5, 9, line=:darkblue, lw=1,label="") # 2nd blue horizontal line
	plot!(x->0.08, -5.5, 9, line=:darkblue, lw=1,label="") # 1st blue horizontal line
	#--------------------------------------------------------------------------------
	# bidirectional vertical red arrows
	plot!([(-5.0,.12),(-5.0,.16)],line=:red,arrow=:both, label="") # 3rd bi arrow
	plot!([(-5.0,.08),(-5.0,.12)],line=:red,arrow=:both, label="") # 2nd bi arrow
	plot!([(-5.0,.00),(-5.0,.08)],line=:red,lw=2,arrow=:both, label="") # 1st bi arrow
	#--------------------------------------------------------------------------------
	# annotations of bidirectional vertical red arrows
	annotate!(-6.0, 0.140, L"\Delta_y", 10)
	annotate!(-6.0, 0.096, L"\Delta_y", 10)
	annotate!(-6.0, 0.045, L"n\Delta y", 10)
	#--------------------------------------------------------------------------------
	# vertical red dashed lines
	plot!([(-3.89,.08),(-3.9,.0)],line=:red, ls=:dash, label="") # 1st vertical dash
	plot!([(-3.76,.12),(-3.76,.0)],line=:red, ls=:dash,label="") # 2nd vertical dash
	plot!([(-2.25,.12),(-2.25,.00)],line=:red, ls=:dash,label="")# 3rd vertical dash
	plot!([(-2.1,.08),(-2.1,.00)],line=:red, ls=:dash, label="") # 4th vertical dash
	plot!([( 1.7,.12),( 1.7,.00)],line=:red,ls=:dash,label="")   # 5th vertical dash
	plot!([( 4.3,.12),( 4.3,.00)],line=:red,ls=:dash,label="")   # 6th vertical dash
	#--------------------------------------------------------------------------------
	# pink rectangles demarking subareas
	plot!(x->.08,-3.9,-3.75,line=:darkblue,fill=(0,:pink),framestyle=:semi,label="")
	plot!(x->.08,-2.25,-2.1,line=:darkblue,fill=(0,:pink),framestyle=:semi,label="")
	plot!(x->.08, 1.7, 4.3,line=:darkblue,fill=(0,:pink),framestyle=:semi, label=L"\mu_{[a,b]}(\{x|n\Delta y) < f(x)\; \leq (n+1)\Delta y\})n\Delta y")
	#--------------------------------------------------------------------------------
	# red horizontal lines demarking length measures
	plot!(x->0.00, -3.9,-3.75,line=:red,lw=3,label="")          # 1st horizontal line
	plot!(x->0.00,-2.25,-2.10,line=:red,lw=3,label="")          # 3rd horizontal line
	plot!(x->0.0, 1.7, 4.3,line=:red,lw=3,label="")             # 4th horizontal line
	#--------------------------------------------------------------------------------
	# red vertical lines demarking Δy
	plot!([(-3.9,.08),(-3.9,.00)],lw=2,line=:red, label="")     # 1st vertical line	
	plot!([(-2.25,.08),(-2.25,.00)],lw=2,line=:red, label="")   # 2th vertical line
	plot!([( 1.7,.08),( 1.7,.00)],lw=2,line=:red, label="")     # 3th vertical line
	#--------------------------------------------------------------------------------
	# annotation
	annotate!(6.70,-0.03, L"\mu_{[a,b]}(\{x|n\Delta y) < f(x)\; \leq (n+1)\Delta y\})", 10)
	plot!([(-3.84,-.03),(-3.84,-.00)],line=:red,arrow=:head, label="") # 1st up arrow
	plot!([(-2.15,-.03),(-2.15,-.00)],line=:red,arrow=:head, label="") # 2nd up arrow
	plot!([( 3.00,-.03),( 3.00,-.00)],line=:red,arrow=:head, label="") # 3rd up arrow
	plot!(x->-.03,-3.8, 3.30,line=:red, label="")              # 4th horizontal line
	#--------------------------------------------------------------------------------
end # function plotLebesguesIntegralBySumOfSteps

# ╔═╡ 5977055b-55d3-4516-87ce-735c9f723d93
plotLebesguesIntegralBySumOfSteps()

# ╔═╡ 7e0a4d61-c190-47ac-a282-5ace1aa7bccd
#------------------------------------------------------------------------------------
# lebesgueIntegralIter2 only integrates *positive* functions
# - integration method is summation of all rectangles with base 
#   μ({x|y < f(x) ≤ y + Δy}) and height f(x) = nΔy. 
#   So the area of this rectangle is: μ({x|nΔy < f(x) ≤ (n+1)Δy})nΔy
# - *non*recursive function earchForMax using *while*
#------------------------------------------------------------------------------------
function lebesgueIntegralIter2(f::Function; a=0.0, b=1.0, y=0.0, Δy=0.01)
	#--------------------------------------------------------------------------------
	Δx = Δy
	add_Δx(x) = x + Δx
	add_Δy(y) = y + Δy	#--------------------------------------------------------------------------------
	function μ(y)
		let μΔx(x) = (add_Δy(y) > f(x) ≥ y) ? Δx : 0
			sum(μΔx(x) for x in a:Δx:b)
		end # let
	end # function μ
	#--------------------------------------------------------------------------------
	function searchForMaxIter(f, fmax, x, b) 
		xmax = x
		while !(add_Δx(x) > b)
			x = add_Δx(x)
			fnew = f(x)                           # new tentative maximum
			if 	fmax < fnew 
				begin
					fmax = fnew
					xmax = x
				end # begin
			end # if
			x = add_Δx(x)
		end # while
		fmax, xmax
	end # function searchForMaxIter
	#--------------------------------------------------------------------------------
	# the search for maximum of function f
	fmax, xmax = searchForMaxIter(f, f(a), a, b)   
	# sum of area of vertical rectangles (=steps) μ({x|nΔy < f(x) ≤ (n+1)Δy})nΔy
	# y = f(x) = nΔy
	# μ(y) = μ(nΔy = μ({x|nΔy < f(x) ≤ (n+1)Δy})
	sumIter(y -> y*μ(y), 0::Real, add_Δy::Function, fmax::Real) 
	#--------------------------------------------------------------------------------
end # function lebesgueIntegralIter2

# ╔═╡ e92c0dad-a4f8-4bcd-aea7-ba45d017c93c
md"
---
###### Integral of Pyramidal Density
"

# ╔═╡ d699d590-da60-4b75-82ae-2827885e5617
lebesgueIntegralIter2(unitPyramid, a=0.0, b=2.0, Δy=0.1E-3)

# ╔═╡ 20102c71-5dec-4e02-a828-a9bb41eb56e1
lebesgueIntegralIter2(unitPyramid, a=0.0, b=2.0, Δy=0.1E-4)

# ╔═╡ db39cefe-ba36-4768-a976-008876ae5293
md"
---
###### Integral of Gaussian Density
"

# ╔═╡ 7ec3f7c2-6023-4cf7-8513-528234c6062d
lebesgueIntegralIter2(gaussianDensity, a=-9, b=0.0, Δy=1.0E-4)

# ╔═╡ 32a8edf5-b98e-470e-ae6d-f99e8234500e
lebesgueIntegralIter2(gaussianDensity, a=-1.0, b=+1.0, Δy=1.0E-4)

# ╔═╡ e22fa140-df8f-4ed9-81b5-f1d340690425
md"
---
###### Integral of Mixed Gaussian Density
"

# ╔═╡ 8b690246-6b46-4232-a981-67fcaac62e14
lebesgueIntegralIter2(mixedDensity, a=-4.0, b=6.5, Δy=1.0E-3)

# ╔═╡ b55764bf-6a1a-4b8d-a9b1-8a1cabb27853
lebesgueIntegralIter2(mixedDensity, a=-4.0, b=6.5, Δy=1.0E-4)

# ╔═╡ c5e7ffcd-d615-4fc0-8de0-0cdb97a76472
