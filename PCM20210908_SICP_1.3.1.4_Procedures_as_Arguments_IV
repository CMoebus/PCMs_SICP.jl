	[0.0742588, 0.0870487, 0.507737, 0.329387],       # TFCM predictions
	[0.0966776, 0.0287166, 0.519706, 0.355])          # DPMVM predictions

# ╔═╡ 8a1c25a5-e091-49f7-8455-cc90b769a4d8
md"
*Both* models show *strong agreement* in predicting first choices at least for this example. The *Pearson* product-moment correlation coefficient (Nazarathy & Klok, 2021, p.123) is near 1.00 (!). Empirical studies have to demonstrate their usefulness: the relatively assumption free *DPMVM* or the more demanding *Thurstone* model.
"

# ╔═╡ d05b08a1-79fc-4c92-99c3-e5807cbc5b74
md"
---
##### 4. *First Choice* Models with *Cumulative Density Functions (CDFs)*
Here we have abandoned all numerical *quadrature* algorithms computing *integrals* of Gaussian PDFs in favor of calling *tabulated* Gaussian CDFs. Results should be *identical* to former results.

##### 4.1 *Code* of Service PDFs and CDFs
"

# ╔═╡ feea3a8e-b56b-471a-93fb-d75a7456b1f6
begin
	# distributions for stimuli I, J, K, L
	gaussianI =  Normal( -1.0,   3.0)
	gaussianJ =  Normal( +1.0,   1.0)
	gaussianK =  Normal( +3.0,   2.0)
	    unitL = Uniform(-10.0, +10.0)
	# densities for stimuli I, J, K, L
	gaussianDensityPDF_I(x) = pdf( gaussianI, x)
	gaussianDensityPDF_J(x) = pdf( gaussianJ, x)
	gaussianDensityPDF_K(x) = pdf( gaussianK, x)
	unitStepDensityPDF_L(x) = pdf( unitL, x)
	# cumulative densities for stimuli I, J, K, L
	gaussianDensityCDF_I(x) = cdf( gaussianI, x)
	gaussianDensityCDF_J(x) = cdf( gaussianJ, x)
	gaussianDensityCDF_K(x) = cdf( gaussianK, x)
	unitStepDensityCDF_L(x) = cdf( unitL, x)
	# inverse of cumulative densities for stimuli I, J, K, L
	invGaussianDensityCDF_I(x) = (1.0 - gaussianDensityCDF_I(x))
	invGaussianDensityCDF_J(x) = (1.0 - gaussianDensityCDF_J(x))
	invGaussianDensityCDF_K(x) = (1.0 - gaussianDensityCDF_K(x))
	invUnitStepDensityCDF_L(x) = (1.0 - unitStepDensityCDF_L(x))
end # begin

# ╔═╡ 54a2b8b7-4b89-4d4c-ae88-9229feedb8e4
function myDensityPDF(m) 
	#-------------------------------------------------------
	m == 1 ? gaussianDensityPDF_I :
	m == 2 ? gaussianDensityPDF_J :
	m == 3 ? gaussianDensityPDF_K :
	m == 4 ? unitStepDensityPDF_L :
		error("undefined case $m in function myDensityPDF")
end # function myDensityPDF

# ╔═╡ 246a25f0-9917-45ae-910e-a23bd508f38f
function myDensityCDF(m) 
	#-------------------------------------------------------
	m == 1 ? gaussianDensityCDF_I :
	m == 2 ? gaussianDensityCDF_J :
	m == 3 ? gaussianDensityCDF_K :
	m == 4 ? unitStepDensityCDF_L :
		error("undefined case $m in function myDensityCDF")
end # function myDensityPDF

# ╔═╡ 3f2d3cbe-032d-4d60-b3e2-b642dcbf986d
md"
---
##### 4.2 *Code* and *Predictions* of *TFCM*
"

# ╔═╡ ec8d7008-97aa-49c2-8143-19ea9d4eafe2
function thurstoneChoiceModelDistrib(;kS=3, a=-10.0, b=+10.0, Δx=0.001) 
	pI = pJ = pK = pK = pL = x->x
	prefProbI = prefProbJ = prefProbK = prefProbL = 0.0
	#-----------------------------------------------------------------------------
	function preferenceDensity(m, pm::Function)
		if m == 1 
			pI = pm 
		elseif m == 2 
			pJ = pm 
		elseif m == 3 
			pK = pm 
		elseif m == 4 
			pL = pm 
		else error("undefined case $m")
		end # if
		pI, pJ, pK, pL             # four preference functions
	end # function preferenceDensity
	#-----------------------------------------------------------------------------
	for i in 1:kS
		for j in 1:kS
			if !(j == i)
				for k in 1:kS
					if !((k == i) || (k == j))
						#-----------------------------------------------
						if kS == 3
							pI, pJ, pK, pL = 
								preferenceDensity(i, x -> 
									myDensityPDF(i)(x) * 
									myDensityCDF(j)(x) * 
									myDensityCDF(k)(x))
						end # if kS == 3
						#-----------------------------------------------
						if kS == 4
							for l in 1:kS
								if !((l == i) || (l == j) || (l == k))
									pI, pJ, pK, pL = 
										preferenceDensity(i, x -> 
											myDensityPDF(i)(x) *
											myDensityCDF(j)(x) * 
											myDensityCDF(k)(x) *
											myDensityCDF(l)(x))
								end # if 
							end # for l
						end # if kS == 4
						#-----------------------------------------------
					end # if k
				end # for k
			end # if j
		end # for j
		#-------------------------------------------------------------------------
		if i == 1
			prefProbI = integralIter(pI, -10, +10, Δx=Δx) # SI
		elseif i == 2
			prefProbJ = integralIter(pJ, -10, +10, Δx=Δx) # SJ
		elseif i == 3
			prefProbK = integralIter(pK, -10, +10, Δx=Δx) # SK
		elseif i == 4
			prefProbL = integralIter(pL, -10, +10, Δx=Δx) # SL
		else
			error("case not defined ")
		end # if
	end # for i
	#-----------------------------------------------------------------------------
	prefProbI, prefProbJ, prefProbK, prefProbL, 
	prefProbI + prefProbJ + prefProbK + prefProbL
end # function thurstoneChoiceModelQuadGK

# ╔═╡ 54fce4f0-7939-4f96-ac3a-eb86fca33171
thurstoneChoiceModelDistrib(kS=3)

# ╔═╡ a15c8538-f3c8-4c14-a8d2-b00072596130
thurstoneChoiceModelDistrib(kS=4)

# ╔═╡ 17603e10-38e5-450f-a431-b40175ca9de7
md"
---
##### 4.3 *Code* and *Predictions* of *DPMVM*
"

# ╔═╡ d78fb2c3-9793-45b7-acbd-a44fbf7eb019
function ourVotingChoiceModelDistrib(;kS=3, b=+10.0, Δx=0.001)
	# initalization to 0.0
	votingProbI = votingProbJ = votingProbK = votingProbL = 0.0
	x = b
	while !(1.0 < (votingProbI + votingProbJ + votingProbK + votingProbL))
		for i in 1:kS
			if i == 1
			 	# votingProbI = integralIter(myDensity(i), x, b, Δx=Δx)
				votingProbI = 1.0 - myDensityCDF(i)(x)
			elseif i == 2
				# votingProbJ = integralIter(myDensity(i), x, b, Δx=Δx)
				votingProbJ = 1.0 - myDensityCDF(i)(x)
			elseif i == 3
				# votingProbK = integralIter(myDensity(i), x, b, Δx=Δx)
				votingProbK = 1.0 - myDensityCDF(i)(x)
			elseif i == 4
				# votingProbL = integralIter(myDensity(i), x, b, Δx=Δx)
				votingProbL = 1.0 - myDensityCDF(i)(x)
			else
				error("unidentified case = $i")
			end # if
		end # for i
		x = x - Δx
	end # while
	votingProbI, votingProbJ, votingProbK, votingProbL, 
	votingProbI + votingProbJ + votingProbK + votingProbL
end # function ourVotingChoiceModelDistrib

# ╔═╡ 846662df-e0c2-4080-a6eb-25ee0e638bba
ourVotingChoiceModelDistrib()

# ╔═╡ 162e20f6-0eb7-4b25-8102-313bd0d170b9
ourVotingChoiceModelDistrib(kS=4)

# ╔═╡ e27371b7-5b48-494f-a006-1ba6a589645d
md"
---
##### 4.4 *Comparison* of Models for 3 Stimuli
"

# ╔═╡ 284120d2-0bb2-4160-a96d-bbe7e6074e31
plotModelComparison("DPMVM vs. TFCM (3 Stimuli) with Distrib.jl", 
	[0.109486, 0.148505, 0.741653],                 # TFCM predictions
	[0.157288, 0.154577, 0.688464])                 # DPMVM predictions

# ╔═╡ 681b6508-4fa0-4a3b-a80b-b2a527edda3d
md"
---
##### 4.4 *Comparison* of Models for 4 Stimuli
"

# ╔═╡ e9b9ea90-f95c-4293-bc25-ed0e9b6cfef9
plotModelComparison("DPMVM vs. TFCM (4 Stimuli) with Distrib.jl", 
	[0.0742391, 0.0871594, 0.508426, 0.32982],      # TFCM predictions
	[0.0967434, 0.028651, 0.51974, 0.35495])        # *DPMVM* predictions

# ╔═╡ c367f470-168a-4cfb-b4ac-49dbe2f59db9
md"
---
##### 5. Stimulus-specific PDFs in *TFCM*
Here we plot the *weighthed* model densities for $S_I, S_J, S_K, S_L$

$f_I(x) \cdot \mu_{x>x_j,x_k,x_l,...}(dx) = f_I(x)\cdot d\mu_{x>x_j,x_k,x_l,...} = f_i(x)\prod_{j=1; j\neq i}^k \int_{-\infty}^x f_j(x)\;dx.$

$\;$
$\;$
$\;$

The area under these densities is equal to the *stimulus-specific* probability of *first choice* vote

$p(I > J,K,...) = \int_{-\infty}^{+\infty} f_I(x) \cdot \mu_{x>x_j,x_k,x_l,...}(dx) = \int_{-\infty}^{+\infty} f_I(x)\cdot d\mu_{x>x_j,x_k,x_l,...}.$

$\;$
$\;$
$\;$

"

# ╔═╡ 78987082-90cc-4d4a-9922-2b0fe1fc8e2e
function integralOfStimulusSpecificPDFsWithQuadGK(f; a=-10.0, b=+10.0) 
	I,est = quadgk(f, a, b, rtol=1e-8)
end # function myIntegralWithQuadGK

# ╔═╡ e3fe5e15-c17f-45d9-9631-cdc5785fed71
md"
---
##### 5.1 for $kS=3$ Stimuli
###### 5.1.1 Stimulus-specificPDFs
"

# ╔═╡ 39ea5482-74bf-4d22-ad2c-76930d7fcafe
function plotThurstonePreferenceDensity(;kS=3)
	xs = [x for x in -10.0:0.1:+10.0]
	if kS == 3 
		plot(x -> gaussianDensityPDF_I(x) * gaussianDensityCDF_J(x) * gaussianDensityCDF_K(x), xs, label=L"P(I > J,K)", title="TFCM: Stimulus-specific PDFs for 3 Stimuli", titlefontsize=12)
		plot!(x -> gaussianDensityPDF_J(x) * gaussianDensityCDF_I(x) * gaussianDensityCDF_K(x), xs, label=L"P(J > I,K)")
		plot!(x -> gaussianDensityPDF_K(x) * gaussianDensityCDF_I(x) * gaussianDensityCDF_J(x), xs, label=L"P(K > I,J)")
	elseif kS == 4
		plot(x -> gaussianDensityPDF_I(x) * gaussianDensityCDF_J(x) * gaussianDensityCDF_K(x) * unitStepDensityCDF_L(x), xs, label=L"P(I > J,K,L)", title="TFCM: Stimulus-specific PDFs for 4 Stimuli", titlefontsize=12)
		plot!(x -> gaussianDensityPDF_J(x) * gaussianDensityCDF_I(x) * gaussianDensityCDF_K(x) * unitStepDensityCDF_L(x), xs, label=L"P(J > I,K,L)")
		plot!(x -> gaussianDensityPDF_K(x) * gaussianDensityCDF_I(x) * gaussianDensityCDF_J(x) * unitStepDensityCDF_L(x), xs, label=L"P(K > I,J,L)")
		plot!(x -> unitStepDensityPDF_L(x) * gaussianDensityCDF_I(x) * gaussianDensityCDF_J(x) * gaussianDensityCDF_K(x), xs, label=L"P(L > I,J,K)")
	end # if 
end # function plotThurstonePreferenceDensity

# ╔═╡ c7d4d038-e54d-4b3b-a075-904f696e3bd5
plotThurstonePreferenceDensity()

# ╔═╡ 0a099648-e4d3-4322-b6b4-2adb50ef0ca7
md"
$p(K > I, J) = 0.741653 > P(J > I, K) = 148505 > p(I > J, K) = 0.109486$

$\;$
$\;$

"

# ╔═╡ c6c03cbf-2fbf-49da-9b70-d8856631859b
thurstoneChoiceModelDistrib(kS=3)   # p(I > J, K), P(J > I, K), p(K > I, J)

# ╔═╡ c0235c94-36cf-4e6b-bd2e-5114a67755bd
md"
###### 5.1.2 Integration of *Stimulus-specific* PDFs Generates *First Choice Probabilities*
"

# ╔═╡ 6f21d546-879e-47fe-ac98-99289d93d612
integralOfStimulusSpecificPDFsWithQuadGK(x -> gaussianDensityPDF_I(x) * gaussianDensityCDF_J(x) * gaussianDensityCDF_K(x))[1]       # p(I > J,K)

# ╔═╡ a0e552e2-3df4-4289-b7b1-ab4e729145ab
integralOfStimulusSpecificPDFsWithQuadGK(x -> gaussianDensityPDF_J(x) * gaussianDensityCDF_I(x) * gaussianDensityCDF_K(x))[1]       # p(J > I,K)

# ╔═╡ 580ccd5d-1aab-4a94-8884-349ffd10347b
integralOfStimulusSpecificPDFsWithQuadGK(x -> gaussianDensityPDF_K(x) * gaussianDensityCDF_I(x) * gaussianDensityCDF_J(x))[1]       # p(K > I,J)

# ╔═╡ af856d5e-b6ed-4212-af3a-29fe63fc9a9f
md"
---
##### 5.2 for $kS=4$ Stimuli
###### 5.2.1 Stimulus-specificPDFs
"

# ╔═╡ 1b5a154e-f30d-4b25-a6f3-c0ca67a26d38
