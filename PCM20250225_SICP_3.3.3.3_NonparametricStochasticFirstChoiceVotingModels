# ╔═╡ d5b6a7ed-c260-4f6e-ba3e-a94118445fe3
md"
---
##### 3.4 *Nonparametric Stochastic DPMVM*

" 

# ╔═╡ a05589d8-ea28-49d0-b34e-55c48fbfbad1
md"
---
###### 3.4.1 *Code* of *Nonparametric DPMVM*
"

# ╔═╡ d6f51c28-7320-4394-b267-a01a6a5488e9
function nonparametricDPMVM(x::AbstractMatrix{Float64}, title; plimit=1.0)
	let (nRows, nCols) = size(x)
		m = nRows + 1                             # number of rating categories
		k = nCols                                 # number of stimuli
		sumOfProbs = zeros(Float64, k)
		while !(sum(sumOfProbs) > plimit || m <= 1)
			m -= 1
			for j in 1:k
				sumOfProbs[j] += x[m, j]
			end # for j
		end # while
		mCrit = m
		sumOfProbs = map(x -> x/sum(sumOfProbs), sumOfProbs) # normalization of probs
		title, mCrit, sumOfProbs, sum(sumOfProbs)
	end # let
end # function nonparametricDPMVM

# ╔═╡ 5bf1a243-9827-4ae2-9f22-f5604ea29f24
md"
---
###### 3.4.2 Applications of *DPMVM* 
###### ... to Thurstone's 1st Artificial Data Set
(Thurstone, 1945, Table 1, p.245)
"

# ╔═╡ 4a4d3c1d-cfe7-4413-902c-1c113f758cf0
plotRatingDistributions(x1, "DPMVM: Thurstone's 1st Data Example (table 1, S1 - S3)", mCrit=5)

# ╔═╡ 053f2f2d-1008-4885-a1e4-b3633a63dd8e
nonparametricDPMVM(x1, "DPMVM: Thurstone's 1st data set, S1-S3")

# ╔═╡ f94ebbab-58d7-467f-a62f-e8b3486e697c
plotRatingDistributions(x2, "DPMVM: Thurstone's 1st augmented data set, S1-S4", mCrit=6)

