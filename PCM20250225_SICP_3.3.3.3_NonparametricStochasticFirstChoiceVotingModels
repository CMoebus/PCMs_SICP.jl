##### 3.3 *Nonparametric Stochastic TFCVM*

" 

# ╔═╡ e9a84ffc-8c18-4360-83d6-b02c483e5634
md"
---
###### 3.3.1 *Code* of *Nonparametric TFCVM*
"

# ╔═╡ 76e1ce4c-997e-49ef-938d-2d562d823f95
function nonparametricTFCVM(ys::AbstractMatrix{Float64}, title)
	(nRows, nCols) = size(ys)
	m = nRows                                 # number of rating categories
	n = nCols                                 # number of stimuli
	ps = zeros(Float64, n)
	#-------------------------------------------------------------------------------
	function makeCDFs(ys::AbstractMatrix{Float64})              
		cdfs = zeros(Float64, (m, n))
		for t in 1:m
			if t == 1
				for i in 1:n
					cdfs[t, i] += ys[t, i]
				end # for i
			else
				for i in 1:n
					cdfs[t, i] += cdfs[t-1, i] + ys[t, i] # p.245, table 1, cols 5-7
				end # for i
			end # if
		end # for t
		cdfs
	end # function makeCDFs
	#------------------------------------------------------------------------------
	function makeUs(cdfs)
		us = zeros(Float64, (m, n))
		indices = collect(Combinatorics.permutations(1:n))
		for prm in 1:factorial(n)
			i = indices[prm][1]
			j = indices[prm][2]
			k = indices[prm][3]
			if n == 3
				for krow in 2:m
					us[krow, i] = cdfs[krow, j] * cdfs[krow, k]
				end # for krow
			elseif n == 4
				l = indices[prm][4]
				for krow in 2:m
					us[krow, i] = cdfs[krow, j] * cdfs[krow, k] * cdfs[krow, l]
				end # for krow
			elseif n == 5
				l  = indices[prm][4]
				l5 = indices[prm][5]
				for krow in 2:m
					us[krow, i] = cdfs[krow, j] * cdfs[krow, k] * cdfs[krow, l] * cdfs[krow, l5]
			end # for krow
			elseif n == 6
				l  = indices[prm][4]
				l5 = indices[prm][5]
				l6 = indices[prm][6]
				for krow in 2:m
					us[krow, i] = cdfs[krow, j] * cdfs[krow, k] * cdfs[krow, l] * cdfs[krow, l5] * cdfs[krow, l6]
				end # for krow
			else
				error("unknown case")
			end # if
		end # for prm
		us
	end # function makeUs
	#-------------------------------------------------------------------------------
	function makeZs(ys)
		zs = zeros(Float64, (m, n))
		for i in 1:n
			for krow in 1:m
				if krow == m
					zs[krow, i] = ys[krow, i]
				else
				    zs[krow, i] = ys[krow, i] + ys[krow+1, i]
				end # if
			end # for krow
		end # for i
		zs
	end # function makeZs
	#-------------------------------------------------------------------------------
	cdfs = makeCDFs(ys)
	us   = makeUs(cdfs)
	zs   = makeZs(ys)
	for i in 1:n
		ps[i] = sum([zs[krow, i] * us[krow, i] for krow = 1:m])/2
	end # for i
	# ys, cdfs, us, zs, ps
	psNormalized = ps ./ sum(ps)
	(ps, sum(ps)), (psNormalized, sum(psNormalized))
end # function nonparametricTFCVM

# ╔═╡ 033fe347-08a5-4f51-92f9-5cae98660cab
md"
